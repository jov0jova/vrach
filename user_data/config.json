{
  "strategy": "vrach_V1",
  "add_config_files": [
    "private_config.json"
  ],
    "dry_run": true,
    "dry_run_wallet": 1000,
    "exchange": {
        "name": "binance",
        "key": "PASTE_YOUR_API_KEY",
        "secret": "PASTE_YOUR_SECRET_KEY",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": ["BTC/USDT", "ETH/USDT", "SOL/USDT", "BNB/USDT", "MATIC/USDT"],
        "pair_blacklist": ["*BTC/BNB*", "*UP*", "*DOWN*"]
    },
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "fiat_display_currency": "USD",
    "timeframe": "5m",
